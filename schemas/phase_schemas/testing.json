{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Testing schema",
  "type": "object",
  "properties": {
    "phase": {
      "type": "string"
    },
    "output": {
      "type": "object",
      "properties": {
        "test_plan": {
          "type": "string"
        },
        "test_cases": {
          "type": "array"
        }
      }
    },
    "tokens_in": {
      "type": "number"
    },
    "tokens_out": {
      "type": "number"
    },
    "model": {
      "type": "string"
    },
    "provider": {
      "type": "string"
    }
  },
  "required": [
    "phase",
    "output"
  ]
}