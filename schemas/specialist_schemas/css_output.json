{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "css_output",
  "type": "object",
  "properties": {
    "design_tokens": {
      "type": "object"
    },
    "utilities": {
      "type": "string"
    },
    "components": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "css": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "css"
        ]
      }
    },
    "notes": {
      "type": "string"
    }
  },
  "required": [
    "components"
  ]
}